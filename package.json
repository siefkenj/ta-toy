{
    "private": true,
    "name": "ta-toy",
    "description": "Toy application to learn how to use Vue.js",
    "license": "MIT",
    "author": "Run Zhao Zeng <run.zeng@mail.utoronto.ca>",
    "version": "1.0.0",
    "scripts": {
        "build":
            "cross-env NODE_ENV=production webpack --progress --hide-modules",
        "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
        "start":
            "concurrently --kill-others-on-fail \"npm run serve\" \"npm run watch\"",
        "watch": "webpack --watch",
        "serve": "cd dist && php -S 127.0.0.1:3000",
        "test": "mocha-webpack test/* --recursive --require test/.setup"
    },
    "dependencies": {
        "axios": "^0.18.0",
        "vue": "^2.5.11",
        "vue-router": "^3.0.1"
    },
    "devDependencies": {
        "@vue/test-utils": "^1.0.0-beta.16",
        "avoriaz": "^6.3.0",
        "babel-core": "^6.26.0",
        "babel-loader": "^7.1.2",
        "babel-preset-env": "^1.6.0",
        "babel-preset-stage-3": "^6.24.1",
        "chai": "^4.1.2",
        "concurrently": "^3.5.1",
        "copy-webpack-plugin": "^4.5.1",
        "cross-env": "^5.0.5",
        "css-loader": "^0.28.7",
        "es6-promise": "^4.2.4",
        "file-loader": "^1.1.4",
        "isomorphic-fetch": "^2.2.1",
        "jsdom": "^11.10.0",
        "jsdom-global": "^3.0.2",
        "mocha": "^5.1.1",
        "mocha-webpack": "^1.1.0",
        "test-utils": "^1.1.1",
        "vue-loader": "^13.0.5",
        "vue-template-compiler": "^2.4.4",
        "webpack": "^3.6.0",
        "webpack-dev-server": "^2.9.1"
    },
    "browserslist": ["> 1%", "last 2 versions", "not ie <= 8"]
}
